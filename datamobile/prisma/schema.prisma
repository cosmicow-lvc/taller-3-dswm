// prisma/schema.prisma

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
}

model Venta {
  id       Int      @id @default(autoincrement())
  producto String
  region   String
  monto    Int
  fecha    DateTime @default(now())
}